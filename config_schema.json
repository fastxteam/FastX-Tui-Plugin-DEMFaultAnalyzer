{
  "enabled": {
    "type": "boolean",
    "default": true,
    "description": "是否启用该插件",
    "required": true
  },
  "log_level": {
    "type": "string",
    "default": "INFO",
    "description": "插件的日志级别",
    "choices": ["DEBUG", "INFO", "WARNING", "ERROR", "CRITICAL"],
    "required": false
  },
  "max_history_items": {
    "type": "integer",
    "default": 100,
    "description": "最大历史记录数量",
    "required": false,
    "min": 10,
    "max": 1000
  },
  "enable_auto_save": {
    "type": "boolean",
    "default": false,
    "description": "是否自动保存分析结果",
    "required": false
  }
}